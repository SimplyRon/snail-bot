{
  "name": "snail-bot",
  "version": "1.0.0",
  "description": "a moderation and logging bot made for the snail discord server",
  "main": "dist/index.js",
  "scripts": {
    "start_noinstall": "tsc && node .",
    "start_nobuild": "node install && node .",
    "start": "npm install && npm run lint && tsc && node .",
    "production": "npm install --only=production && node.",
    "lint": "eslint . --ext .ts",
    "dev": "cross-conf-env docker-compose build --parallel && cross-conf-env docker-compose up --abort-on-container-exit --no-deps"
  },
  "author": "ElementalMP4",
  "repository": {
    "type": "git",
    "url": "git://github.com/SimplyRon/snail-bot.git"
  },
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "discord.js": "^12.5.1",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-formatter-github": "^1.0.11",
    "cross-conf-env": "^1.2.1"
  }
}
